<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for products -->

        <record id="seq_product_product" model="ir.sequence">
            <field name="name">Product</field>
            <field name="code">product.template</field>
            <field name="prefix">PRD</field>
